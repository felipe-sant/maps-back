{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "out",
    "builds": [
        {
            "src": "out/src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "out/src/app.js"
        }
    ]
}